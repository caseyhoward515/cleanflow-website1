{
  "redirects": [
    {
      "source": "/services",
      "destination": "/services.html",
      "permanent": true
    },
    {
      "source": "/about",
      "destination": "/about.html",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "cleanflowohio.com"
        }
      ],
      "destination": "https://www.cleanflowohio.com/:path*",
      "permanent": true
    }
  ]
}